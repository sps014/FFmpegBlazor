    <PackageProjectUrl>https://github.com/sps014/FFmpegBlazor</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sps014/FFmpegBlazor</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>sps014</Authors>
    <AssemblyVersion>1.0.0.11</AssemblyVersion>
    <FileVersion>1.0.0.11</FileVersion>
